{
  "name": "@adonisjs/presets",
  "version": "0.0.0",
  "description": "A collection of presets to configure AdonisJS packages",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npm run quick:test",
    "lint": "eslint . --ext=.ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "quick:test": "node --loader ts-node/esm --enable-source-maps bin/test.ts"
  },
  "keywords": [
    "adonisjs",
    "presets"
  ],
  "author": "virk,adonisjs",
  "license": "MIT",
  "dependencies": {
    "@poppinss/utils": "^6.7.0"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^6.1.3-29",
    "@adonisjs/core": "^6.1.5-33",
    "@adonisjs/eslint-config": "^1.2.0",
    "@adonisjs/prettier-config": "^1.2.0",
    "@adonisjs/tsconfig": "^1.2.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@japa/assert": "^2.0.0-2",
    "@japa/file-system": "^2.1.0",
    "@japa/runner": "^3.0.0-9",
    "@japa/snapshot": "^2.0.0-2",
    "@swc/core": "^1.3.100",
    "c8": "^8.0.1",
    "dedent": "^1.5.1",
    "husky": "^8.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "prettier": "@adonisjs/prettier-config",
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
