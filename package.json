{
  "name": "@adonisjs/presets",
  "version": "2.6.0",
  "description": "A collection of presets to configure AdonisJS packages",
  "type": "module",
  "files": [
    "build",
    "!build/bin",
    "!build/tests"
  ],
  "exports": {
    "./lucid": "./build/src/lucid/main.js",
    "./auth": "./build/src/auth/main.js"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "c8 npm run quick:test",
    "lint": "eslint . --ext=.ts",
    "clean": "del-cli build",
    "copy:templates": "copyfiles \"src/**/stubs/**/*.stub\" build",
    "typecheck": "tsc --noEmit",
    "precompile": "npm run lint && npm run clean",
    "compile": "tsup-node && tsc --emitDeclarationOnly --declaration",
    "postcompile": "npm run copy:templates",
    "build": "npm run compile",
    "release": "npx release-it",
    "version": "npm run build",
    "sync-labels": "github-label-sync --labels .github/labels.json adonisjs/presets",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "quick:test": "node --loader ts-node/esm --enable-source-maps bin/test.ts"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.7.0",
    "@adonisjs/core": "^6.11.0",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@japa/assert": "^3.0.0",
    "@japa/file-system": "^2.3.0",
    "@japa/runner": "^3.1.4",
    "@japa/snapshot": "^2.0.5",
    "@poppinss/cliui": "^6.4.1",
    "@swc/core": "^1.6.1",
    "c8": "^10.1.2",
    "copyfiles": "^2.4.1",
    "dedent": "^1.5.3",
    "del-cli": "^5.1.0",
    "github-label-sync": "^2.3.1",
    "husky": "^9.0.11",
    "release-it": "^17.2.1",
    "timekeeper": "^2.3.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@poppinss/utils": "^6.7.3"
  },
  "peerDependencies": {
    "@adonisjs/assembler": "^7.4.0",
    "@adonisjs/core": "^6.5.0"
  },
  "peerDependenciesMeta": {
    "@adonisjs/assembler": {
      "optional": true
    }
  },
  "author": "virk,adonisjs",
  "license": "MIT",
  "homepage": "https://github.com/adonisjs/presets#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/presets.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/presets/issues"
  },
  "keywords": [
    "adonisjs",
    "presets"
  ],
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): ${version}",
      "tagAnnotation": "v${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}",
      "web": true
    }
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "tsup": {
    "entry": [
      "index.ts",
      "./src/auth/main.ts",
      "./src/lucid/main.ts"
    ],
    "outDir": "./build",
    "clean": true,
    "format": "esm",
    "dts": false,
    "sourcemap": true,
    "target": "esnext"
  }
}
