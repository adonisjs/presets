{
  "name": "@adonisjs/presets",
  "version": "2.0.0",
  "description": "A collection of presets to configure AdonisJS packages",
  "type": "module",
  "files": [
    "build",
    "!build/bin",
    "!build/tests"
  ],
  "exports": {
    "./lucid": "./build/src/lucid/main.js",
    "./auth": "./build/src/auth/main.js"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "c8 npm run quick:test",
    "lint": "eslint . --ext=.ts",
    "clean": "del-cli build",
    "copy:templates": "copyfiles \"src/**/stubs/**/*.stub\" build",
    "typecheck": "tsc --noEmit",
    "precompile": "npm run lint && npm run clean",
    "compile": "tsup-node && tsc --emitDeclarationOnly --declaration",
    "postcompile": "npm run copy:templates",
    "build": "npm run compile",
    "release": "np",
    "version": "npm run build",
    "sync-labels": "github-label-sync --labels .github/labels.json adonisjs/presets",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "quick:test": "node --loader ts-node/esm --enable-source-maps bin/test.ts"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.0.0-0",
    "@adonisjs/core": "^6.1.5-34",
    "@adonisjs/eslint-config": "^1.2.0",
    "@adonisjs/prettier-config": "^1.2.0",
    "@adonisjs/tsconfig": "^1.2.0",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@japa/assert": "^2.0.0-2",
    "@japa/file-system": "^2.1.1",
    "@japa/runner": "^3.0.0-9",
    "@japa/snapshot": "^2.0.0-2",
    "@poppinss/cliui": "^6.2.3",
    "@swc/core": "^1.3.100",
    "c8": "^8.0.1",
    "copyfiles": "^2.4.1",
    "dedent": "^1.5.1",
    "del-cli": "^5.1.0",
    "github-label-sync": "^2.3.1",
    "husky": "^8.0.3",
    "np": "^9.2.0",
    "timekeeper": "^2.3.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@poppinss/utils": "^6.7.0"
  },
  "peerDependencies": {
    "@adonisjs/assembler": "^7.0.0-0",
    "@adonisjs/core": "^6.1.5-34"
  },
  "author": "virk,adonisjs",
  "license": "MIT",
  "homepage": "https://github.com/adonisjs/presets#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/presets.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/presets/issues"
  },
  "keywords": [
    "adonisjs",
    "presets"
  ],
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "np": {
    "message": "chore(release): %s",
    "tag": "latest",
    "branch": "main",
    "anyBranch": false
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "tsup": {
    "entry": [
      "index.ts",
      "./src/auth/main.ts",
      "./src/lucid/main.ts"
    ],
    "outDir": "./build",
    "clean": true,
    "format": "esm",
    "dts": false,
    "sourcemap": true,
    "target": "esnext"
  }
}
